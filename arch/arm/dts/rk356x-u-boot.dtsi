#include "rockchip-u-boot.dtsi"

/ {
	aliases {
		mmc0 = &sdhci;
		mmc1 = &sdmmc0;
	};

	chosen {
		u-boot,spl-boot-order = &sdhci, &sdmmc0;
	};

	dmc: dmc {
		compatible = "rockchip,rk3568-dmc";
		bootph-all;
		status = "okay";
	};
};

&cru {
	bootph-all;
	status = "okay";
};

&pmucru {
	bootph-all;
	status = "okay";
};

&grf {
	bootph-all;
	status = "okay";
};

&pmugrf {
	bootph-all;
	status = "okay";
};

&sdhci {
	bootph-pre-ram;
	status = "okay";
};

&sdmmc0 {
	bootph-pre-ram;
	status = "okay";
};
