/ {
	//title = "Test Configuration";
	title-id = <ID_TITLE>;
	prompt = "UP and DOWN to choose, ENTER to select";
	prompt-id = <ID_PROMPT>;
	scenes {
		main {
			id = <ID_SCENE1>;
			title = "Title";
			title-id = <ID_SCENE1_TITLE>;

			cpu-speed {
				type = "menu";
				title = "Test menu";
				title-id = <ID_CPU_SPEED_TITLE>;

				items = "one", "two", "three";
				item-ids = <ID_CPU_SPEED_1 ID_CPU_SPEED_2
					ID_CPU_SPEED_3>;
			};
		};
	};

	strings {
		en-nz {
			fallack = "en";
		};
		en {
			title {
				id = <ID_TITLE>;
				value = "Test Configuration";
			};
		};
	};
};
