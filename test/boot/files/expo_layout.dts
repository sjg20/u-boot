/*
 * Sample expo screen layout
 */

/dts-v1/;

/*
enum {
	ID_TITLE,
	ID_PROMPT,

	ID_SCENE1,
	ID_SCENE1_TITLE,
	ID_CPU_SPEED_TITLE,
	ID_CPU_SPEED_1,
	ID_CPU_SPEED_2,
	ID_CPU_SPEED_3,
};
